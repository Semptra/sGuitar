<controls:StartupWindowControl x:Class="sGuitar.UI.StartupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sGuitar.UI"
        xmlns:reactiveUi="http://reactiveui.net"
        xmlns:controls="clr-namespace:sGuitar.UI.Controls"
        mc:Ignorable="d"
        Title="sGuitar" Width="1600" Height="800">
    <reactiveUi:RoutedViewHost HorizontalContentAlignment="Stretch"
                               VerticalContentAlignment="Stretch"
                               Router="{Binding Router}" />
</controls:StartupWindowControl>
